//6.Write a program for matrix addition
import java.io.*;
import java.util.*;
public class sum
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter two numbers:");
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        int sum=n1+n2;
        System.out.println("sum:"+sum);
        
        
    }
}
//output:
enter two numbers:
4
544
sum:548
